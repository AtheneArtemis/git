.sq-zhaoc {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #000;
  opacity: 0.7;
}

.sq-code-box {
  position: fixed;
  text-align: center;
  background: #fff;
  width: 90%;
  left: 5%;
  top: 15%;
  border-radius: 16rpx;
  z-index: 999;
}

.sq-code-text {
  margin: 30rpx 0;
}
.sq-confirm-btn {
  margin: 30rpx auto;
  /* line-height: 60rpx; */
  /* width: 194rpx; */
  border-radius: 30rpx;
  font-size: 0.85rem;
  background-color:#04be01;
  line-height:69rpx;
  width:415rpx;
  color:white;
}

.sq-close {
  color: #333;
  font-size: 0.65rem;
  margin-bottom: 20rpx;
}

.sq-code-box .sq-title {
  font-size: 0.95rem;
  padding: 30rpx;
  text-align: center;
  border-bottom: 1px solid #eee;
}

.sq-ftitle {
  text-align: center;
  padding: 30rpx;
}

.sq-windows {
  width: 458rpx;
  height: 400rpx;
}
.sq-btn{
  background-color:#04be01;
  line-height:69rpx;
  width:415rpx;
}
